{
    "name": "twinkle-api",
    "version": "1.0.0",
    "description": "",
    "main": "dist/twinkle-api.js",
    "author": "Dhanush Mathivanan",
    "repository": {
        "type": "git",
        "url": "master-api"
    },
    "license": "MIT",
    "scripts": {
        "copySwaggerDoc": "cpx \"src/swagger-docs/*.*\" dist/swagger-docs",
        "copyConfig": "cpx \"src/config/*.json\" dist/config",
        "copyAssets": "cpx \"src/assets/*.*\" dist/assets",
        "start": "npm run build:prod && npm run copySwaggerDoc && npm run copyAssets && npm run copyConfig &&  tsc -p ./src && concurrently \"tsc -w -p ./src\" \"nodemon dist/twinkle-api.js\"",
        "lint": "npm run tslint \"src/**/*.ts\"",
        "tslint": "tslint",
        "pree2e": "webdriver-manager update",
        "build:local": "tsc -p ./src",
        "build:prod": "npm run clean:dest && webpack",
        "clean:dest": "rimraf dist",
        "clean:build": "rimraf build",
        "cover": "nyc --reporter=lcov mocha --timeout 10000 --exit --reporter spec --require ts-node/register test/**/*.test.ts",
        "covertest": "nyc npm test",
        "test": "mocha",
        "build:excelexport": "npm run clean:dest && webpack --config webpack_exportexcel.config.js"
    },
    "keywords": [
        "typescript",
        "mocha",
        "sourcemaps",
        "node",
        "npm scripts",
        "tsc"
    ],
    "dependencies": {
        "@okta/jwt-verifier": "0.0.11",
        "async": "^2.6.0",
        "aws-sdk": "^2.493.0",
        "body-parser": "^1.18.2",
        "compression": "^1.7.1",
        "cors": "^2.8.4",
        "cpx": "1.5.0",
        "exceljs": "1.7.0",
        "exportjs": "^0.2.2",
        "express": "^4.16.2",
        "express-winston": "^2.5.1",
        "fs": "0.0.1-security",
        "fs-extra": "^8.1.0",
        "httpreq": "0.4.23",
        "imap": "^0.8.19",
        "imap-simple": "^5.0.0",
        "joi": "^12.0.0",
        "jsontoxml": "^1.0.1",
        "jsonwebtoken": "^7.1.9",
        "jwk-to-pem": "1.2.6",
        "lodash": "^4.17.4",
        "log4js": "^2.4.1",
        "morgan": "^1.9.0",
        "multer": "^1.4.1",
        "mysql2": "^1.6.0",
        "nconf": "0.10.0",
        "nodemailer": "^6.4.8",
        "nodemailer-smtp-transport": "^2.7.4",
        "npm": "^6.14.5",
        "nyc": "^11.4.1",
        "redis": "^3.0.2",
        "remove-accents": "^0.4.2",
        "request": "^2.83.0",
        "rimraf": "2.6.2",
        "semver": "^6.3.0",
        "sequelize": "^5.21.10",
        "socket.io": "^2.1.0",
        "sqlite3": "^4.0.3",
        "swagger-express-router": "1.0.0",
        "swagger-jsdoc": "1.9.7",
        "swagger-ui-express": "2.0.14",
        "tedious": "^6.2.0",
        "ts-loader": "3.2.0",
        "winston": "3.2.1",
        "winston-daily-rotate-file": "4.4.2",
        "xlsx": "^0.16.2",
        "xml2js": "^0.4.19",
        "xoauth2": "^1.2.0"
    },
    "devDependencies": {
        "@types/async": "^2.0.45",
        "@types/bluebird": "^3.5.21",
        "@types/body-parser": "^1.16.8",
        "@types/chai": "^4.0.10",
        "@types/chai-http": "3.0.3",
        "@types/compression": "^0.0.35",
        "@types/cors": "^2.8.3",
        "@types/express": "^4.11.0",
        "@types/lodash": "4.14.106",
        "@types/log4js": "^2.3.5",
        "@types/mocha": "^2.2.44",
        "@types/morgan": "^1.7.35",
        "@types/multer": "^1.3.7",
        "@types/node": "^9.3.0",
        "@types/nodemailer": "^6.4.0",
        "@types/nodemailer-smtp-transport": "^2.7.4",
        "@types/request": "2.0.12",
        "@types/sequelize": "^4.28.9",
        "@types/validator": "^13.0.0",
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "concurrently": "^3.5.1",
        "copy-webpack-plugin": "4.3.1",
        "jsonwebtoken": "^8.1.0",
        "jwk-to-pem": "^1.2.6",
        "mocha": "^5.0.0",
        "mocha-sonarqube-reporter": "^1.0.1",
        "nodemon": "^1.14.1",
        "shelljs": "^0.8.0",
        "sinon": "^4.4.2",
        "ts-node": "^4.0.2",
        "tslint": "^5.8.0",
        "typescript": "3.5.3",
        "uuid": "^3.3.2",
        "webpack": "3.10.0",
        "webpack-node-externals": "1.6.0"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "src/**/api.ts",
            "src/**/user.manager.ts",
            "src/**/httpHelper.ts",
            "src/**/user.router.ts",
            "src/**/okta.manager.ts",
            "src/**/swagger.controller.ts",
            "src/config/*.ts",
            "src/register.routes.ts",
            "src/webApi.ts",
            "src/authenticate.module.ts",
            "src/helpers/sequelize/*.ts",
            "src/helpers/resulthelper.ts",
            "src/interfaces/*.ts",
            "src/entities/*.ts",
            "src/**/logger.ts"
        ],
        "extension": ".ts",
        "sourceMap": true,
        "instrument": true,
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "json",
            "html"
        ],
        "all": true
    }
}
